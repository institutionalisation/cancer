#!/bin/sh
mkdir -p bin
javac -cp lwjgl/lwjgl-stb-natives-linux.jar:lwjgl/lwjgl-glfw-natives-windows.jar:lwjgl/lwjgl-assimp-javadoc.jar:lwjgl/lwjgl-glfw-sources.jar:lwjgl/lwjgl-glfw-javadoc.jar:lwjgl/lwjgl-openal-natives-macos.jar:lwjgl/lwjgl-natives-windows.jar:lwjgl/lwjgl-opengl-sources.jar:lwjgl/lwjgl-opengl-javadoc.jar:lwjgl/lwjgl-stb.jar:lwjgl/lwjgl-glfw.jar:lwjgl/lwjgl-stb-natives-macos.jar:lwjgl/lwjgl-natives-linux.jar:lwjgl/lwjgl-stb-natives-windows.jar:lwjgl/lwjgl-assimp.jar:lwjgl/lwjgl-javadoc.jar:lwjgl/lwjgl-openal.jar:lwjgl/lwjgl-stb-javadoc.jar:lwjgl/lwjgl-assimp-natives-windows.jar:lwjgl/lwjgl-assimp-natives-macos.jar:lwjgl/lwjgl-openal-sources.jar:lwjgl/lwjgl-natives-macos.jar:lwjgl/lwjgl-stb-sources.jar:lwjgl/lwjgl-openal-javadoc.jar:lwjgl/lwjgl-glfw-natives-linux.jar:lwjgl/lwjgl-assimp-sources.jar:lwjgl/lwjgl-assimp-natives-linux.jar:lwjgl/lwjgl-opengl-natives-macos.jar:lwjgl/lwjgl-openal-natives-linux.jar:lwjgl/lwjgl-glfw-natives-macos.jar:lwjgl/lwjgl-opengl.jar:lwjgl/lwjgl-opengl-natives-windows.jar:lwjgl/lwjgl-opengl-natives-linux.jar:lwjgl/lwjgl.jar:lwjgl/lwjgl-sources.jar:lwjgl/lwjgl-openal-natives-windows.jar:lwjgl/lwjgl-stb.jar:joml/joml.jar:src -d bin src/*.java
